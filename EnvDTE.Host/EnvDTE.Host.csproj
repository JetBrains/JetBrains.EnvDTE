<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net461</TargetFramework>
        <LangVersion>latest</LangVersion>
        <AssemblyName>JetBrains.EnvDTE.Host</AssemblyName>
        <RootNamespace>JetBrains.EnvDTE.Host</RootNamespace>
        <OutputPath>..\output\JetBrains.EnvDTE.Host\$(Configuration)\</OutputPath>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="JetBrains.Rider.SDK" Version="$(RiderSDKVersion)">
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>
    </ItemGroup>
</Project>
